<!DOCTYPE html>
<html>
  <head>
    <title>MyEyes</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>

  <body>

  <div class="header">
    <ul class="hmenu">
      <%= link_to image_tag("http://www.clker.com/cliparts/g/C/A/q/j/k/eye-biggest.svg", class: "head-icon"), root_path %>
      <%= render 'devise/menu/registration_items' %>|
      <%= render 'devise/menu/login_items' %>
    </ul>
  </div>

  <%= yield %>

  </body>
</html>
